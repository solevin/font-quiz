{
    "problemMatcher": [
        {
            "owner": "lint-info-checker",
            "severity": "notice",
            "pattern": [
                {
                    "regexp": "^\\s*(info)\\s•\\s(.*)\\s•\\s(.*):(\\d+):(\\d+)\\s•\\s(.*)$",
                    "message": 2,
                    "file": 3,
                    "line": 4,
                    "column": 5,
                    "code": 6
                }
            ]
        },
        {
            "owner": "lint-warning-and-error-checker",
            "pattern": [
                {
                    "regexp": "^\\s*(warning|error)\\s•\\s(.*)\\s•\\s(.*):(\\d+):(\\d+)\\s•\\s(.*)$",
                    "severity": 1,
                    "message": 2,
                    "file": 3,
                    "line": 4,
                    "column": 5,
                    "code": 6
                }
            ]
        },
        {
            "owner": "dart-code-metrics-warning-checker",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^\\s*(WARNING)\\s*(.*)$",
                    "message": 2
                },
                {
                    "regexp": "^\\s*(lib/.*):(\\d*):(\\d)$",
                    "file": 1,
                    "line": 2,
                    "column": 3
                },
                {
                    "regexp": "^\\s*(.*)\\s*:\\s.*$",
                    "code": 1
                }
            ]
        }
    ]
}